{
  "name": "audiovideo",
  "version": "v1.1.1",
  "description": "Cross-platform audio record/playback and video recording.",
  "repository": "tcr/node-audiovideo",
  "main": "index.js",
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "node test/test.js"
  },
  "author": "Tim Cameron Ryan <tim@technical.io>",
  "license": "MIT",
  "dependencies": {
    "bindings": "^1.2.1",
    "jpeg": "git+https://github.com/tcr/node-jpeg.git",
    "nan": "^1.7.0",
    "node-pre-gyp": "0.6.x"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "devDependencies": {
    "imagesize": "^1.0.0",
    "aws-sdk": "~2.0.0-rc.15"
  },
  "binary": {
    "module_name": "capture",
    "module_path": "./src/binding/",
